<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.publicopinion.entity.User">
    <select id="getUsersByUsername" resultType="com.example.publicopinion.entity.User">
        SELECT * FROM users WHERE username LIKE CONCAT('%', #{username}, '%')
    </select>
</mapper>



